<h1>ツイート一覧</h1>
    <div class="top">
        <table class="all_table">
            <tr>
                <th>内容</th>
            </tr>
            <% @blogs.each do |blog| %>
                <tr>
                    <td><%= blog.content%></td>
                        <td><%= link_to '詳細を確認する',blog_path(blog.id),{class:"index_btn_link2"}%></td>
                        <td><%= link_to 'ツイートを編集する',edit_blog_path(blog.id),{class:"index_btn_link2"}%></td>
                        <td><%= link_to 'ツイートを削除する', blog_path(blog.id), method: :delete ,data: { confirm: '本当に削除していいですか？' }, class: "index_btn_link3"%></td>
                </tr>
            <% end %>
        </table>
        <%= link_to '新しくツイートを投稿する', new_blog_path, {class:"index_btn_link4"} %>
    </div>


